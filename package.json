{
  "name": "pawql",
  "version": "0.3.0",
  "description": "The Runtime-First ORM for TypeScript. Type-safe database query builder with zero code generation.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "types": "./dist/index.d.ts"
    },
    "./testing": {
      "import": "./dist/testing.js",
      "types": "./dist/testing.d.ts"
    }
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc -p tsconfig.build.json",
    "prepublishOnly": "npm run build",
    "test": "npx tsx --test test/*.test.ts",
    "test:watch": "npx tsx --test --watch test/*.test.ts",
    "test:bun": "bun test"
  },
  "keywords": [
    "orm",
    "typescript",
    "nodejs",
    "query-builder",
    "database",
    "sql",
    "postgresql",
    "postgres",
    "type-safe",
    "runtime-first",
    "zero-codegen",
    "no-build-step",
    "lightweight-orm",
    "bun"
  ],
  "author": "Ahmat Fauzi",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ahmatfauzy/pawql-orm.git"
  },
  "bugs": {
    "url": "https://github.com/ahmatfauzy/pawql-orm/issues"
  },
  "homepage": "https://github.com/ahmatfauzy/pawql-orm#readme",
  "devDependencies": {
    "@types/bun": "latest",
    "@types/pg": "^8.16.0",
    "tsx": "^4.19.0",
    "typescript": "^5"
  },
  "peerDependencies": {
    "typescript": "^5",
    "pg": "^8.18.0"
  }
}
