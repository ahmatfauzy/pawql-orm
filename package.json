{
  "name": "genless",
  "version": "0.1.0",
  "description": "Runtime-first, type-safe database query builder for Node.js without code generation.",
  "type": "module",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "bun build ./src/index.ts --outdir ./dist --target node && tsc --emitDeclarationOnly --outDir ./dist",
    "prepublishOnly": "bun run build",
    "test": "bun test"
  },
  "keywords": [
    "database",
    "query-builder",
    "typescript",
    "runtime",
    "type-safe"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "typescript": "^5"
  },
  "peerDependencies": {
    "typescript": "^5"
  }
}
